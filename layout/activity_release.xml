<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".release">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="189dp"
        android:layout_height="69dp"
        android:layout_marginTop="36dp"
        android:text="RELEASE THE DOCUMENTS"
        android:textColor="#FFAD09"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imggal"
        android:layout_width="304dp"
        android:layout_height="212dp"
        android:layout_marginTop="138dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:srcCompat="@drawable/common_google_signin_btn_icon_disabled" />

    <Button
        android:id="@+id/btnGallary"
        android:layout_width="206dp"
        android:layout_height="78dp"
        android:layout_marginTop="72dp"
        android:text="UPLOAD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.531"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imggal" />

</androidx.constraintlayout.widget.ConstraintLayout>